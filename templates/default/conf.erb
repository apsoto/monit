# managed by chef, changes will be overwritten
check <%= @type.to_s %> <%= @name %>
  with <% if @type == :process %>pidfile <%= @pid %><% else %>path <%= @path %><% end %>
  <% if @start %>start program = "<%= @start %>"<% end -%>
  <% if @stop %>stop program = "<%= @stop %>"<% end -%>
  <% if @group -%>
  <% Array(@group).each do |g| -%>
  group <%= g %>
  <% end -%>
  <% end -%>
  <% if @rule -%>
  <% Array(@rule).each do |r| -%>
  <%= r %>
  <% end -%>
  <% end -%>
  <% if @depends -%>
  <% Array(@depends).each do |d| -%>
  depends <%= d %>
  <% end -%>
  <% end -%>
